  "scripts": {
    "start": "export HTTPS=true&&SSL_CRT_FILE=./server/ssl/localhost.crt&&SSL_KEY_FILE=./server/ssl/localhost.key&&react-scripts start",
    "start-win": "set HTTPS=true&&SSL_CRT_FILE=./server/ssl/localhost.crt&&SSL_KEY_FILE=./server/ssl/localhost.key&&react-scripts start",
    "react": "export HTTPS=true&&SSL_CRT_FILE=./server/ssl/localhost.crt&&SSL_KEY_FILE=./server/ssl/localhost.key&&react-scripts start",
    "server": "nodemon -r dotenv/config ./server/server.js dotenv_config_path=./.env.development",
    "server-prod": "nodemon -r dotenv/config ./server/server.js dotenv_config_path=./.env",
    "deploy": "react-scripts build && sudo cp -r build/ ../tunehatch/",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "snapup": "CI=true react-scripts test -u",
    "test:client": "react-scripts test",
    "test:server": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test": "npm run test:client -- --watchAll=false && npm run test:server",
    "build:shared": "tsc src/Helpers/shared/*.ts --outDir src/Helpers/shared_dist/ --jsx preserve --esModuleInterop --skipLibCheck --module esnext --moduleResolution node",
    "prebuild": "npm run build:shared",
    "prestart": "npm run build:shared"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
